    <dependencyManagement>
    <dependencies>
        <dependency>
            <groupId>com.hujiang</groupId>
            <artifactId>rocky-common</artifactId>
            <version>${project.rocky.version}</version>
        </dependency>
        <dependency>
            <groupId>com.hujiang</groupId>
            <artifactId>rocky-web</artifactId>
            <version>${project.rocky.version}</version>
        </dependency>
        <dependency>
            <groupId>com.hujiang</groupId>
            <artifactId>rocky-config-client</artifactId>
            <version>${project.rocky.version}</version>
        </dependency>
        <dependency>
            <groupId>com.hujiang</groupId>
            <artifactId>rocky-springboot</artifactId>
            <version>${project.rocky.version}</version>
        </dependency>
        <dependency>
            <groupId>com.hujiang</groupId>
            <artifactId>rocky-rest</artifactId>
